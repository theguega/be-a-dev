{
  "icon_theme": "Catppuccin Latte",
  "theme": {
    "mode": "light",
    "light": "Catppuccin Latte - No Italics",
    "dark": "Catppuccin Latte - No Italics"
  },
  "base_keymap": "VSCode",
  "features": {
    "edit_prediction_provider": "zed"
  },
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 16,
  "buffer_font_weight": 400,
  "buffer_line_height": "comfortable",
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_weight": 400,
  "soft_wrap": "editor_width",
  "ui_font_size": 16,
  "multi_cursor_modifier": "alt",
  "cursor_shape": "bar",
  "cursor_blink": false,
  "tab_bar": {
    "show": false
  },
  "outline_panel": {
    "dock": "right"
  },
  "indent_guides": {
    "coloring": "indent_aware",
    "background_coloring": "indent_aware"
  },
  "project_panel": {
    "dock": "right"
  },
  "agent": {
    "dock": "left"
  },
  "tabs": {
    "close_position": "right"
  },
  "terminal": {
    "default_height": 200,
    "default_width": 400,
    "dock": "bottom",
    "cursor_shape": "bar",
    "toolbar": {
      "breadcrumbs": false
    },
    "working_directory": "current_project_directory"
  },
  "git_panel": {
    "dock": "right"
  },
  "scrollbar": {
    "git_diff": false,
    "cursors": false,
    "search_results": true,
    "selected_symbol": false,
    "selected_text": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "selections_menu": false,
    "agent_review": false,
    "code_actions": false,
    "quick_actions": false
  }
}
